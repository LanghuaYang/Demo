@model IEnumerable<Auckland_High_School.ViewModel.TeacherViewModel>
@{
    ViewBag.Title = "TeacherList";
}

<h2>TeacherList</h2>
<table>
    <tr>
        <th>
            Name
        </th>
        <th>
            Gender
        </th>
        <th>
            TotalSubject
        </th>
        <th>
            TotalClass
        </th>
    </tr>
    @foreach (var s in Model)
    {
        <tr>
            <td>
                @s.FirstName  @s.LastName
            </td>
            <td>
                @s.Gender
            </td>
            <td>
                @s.TotalSubject
            </td>
            <td>
                @s.TotalClass
            </td>  
        </tr>
    }
</table>

@Html.ActionLink("Back To Home Page", "Index", "Home")

