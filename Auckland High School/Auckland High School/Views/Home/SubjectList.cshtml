@model IEnumerable<Auckland_High_School.ViewModel.SubjectViewModel>
@{
    ViewBag.Title = "SubjectList";
}

<h2>SubjectList</h2>
<table>
    <tr>
        <th>
            Name
        </th>
        <th>
            TotalStaff
        </th>
        <th>
            TotalClass
        </th>
        <th>
            TotalEnrollment
        </th>
    </tr>
    @foreach (var s in Model)
    {
        <tr>
            <td>
                @s.Name
            </td>
            <td>
                @s.TotalStaff
            </td>
            <td>
                @s.TotalClass
            </td>
            <td>
                @s.TotalEnrollment
            </td>
        </tr>
    }
</table>
@Html.ActionLink("Back To Home Page", "Index", "Home")
