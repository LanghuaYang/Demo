@model IEnumerable<Auckland_High_School.ViewModel.StudentViewModel>
@{
    ViewBag.Title = "StudentList";
}

<h2>StudentList</h2>
<table>
    <tr>
        <th>
            Name
        </th> 
        <th>
            Gender
        </th> 
        <th>
            TotalEnrollment
        </th>  
    </tr> 
    @foreach (var s in Model)
    {
        <tr>
            <td>
                @s.FirstName  @s.LastName
            </td>
            <td>
                @s.Gender
            </td>
            <td>
                @s.TotalEnrollment
            </td>
        </tr>
    }
</table>

@Html.ActionLink("Back To Home Page", "Index","Home")

